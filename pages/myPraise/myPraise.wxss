/* pages/myPraise/myPraise.wxss */
@import '/pages/template/template.wxss';
page{
  width:100%;
  height: 100%;
  background-color: rgb(247, 247, 247);
}
.allsaysPage{
  height: 100%;
}
scroll-view {
  height: 100%;
}
.allsaysPage > view{
  margin-bottom: 30rpx;
}
.addSays {
  position:fixed;
  top:20rpx;
  right:5rpx
  }
.addSays image {
  width:80rpx;
  height:80rpx;
  }
.addSays-hover{
  width:100rpx;
  height:100rpx;
}
.noteItem{
  margin-bottom: 30rpx;

}

::-webkit-scrollbar {
  display: none;
  width: 0;
  height: 0;
  color: transparent;
}